<% @properties.each do |property| %>
  <p><%= property.title %></p>
  <p><%= property.property_type.name %></p>
  <p><%= property.location  %></p>
  <p><%= property.area %></p>
  <p><%= property.description %></p>
  <p><%= number_to_currency(
      property.daily_rate , unit: "R$ ", separator: ",", delimiter: "")%></p>
  <p><%= property.photo %></p>
<% end %>
